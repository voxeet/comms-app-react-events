    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.8",
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.5",
    "wait-on": "^6.0.1"
  }
}
